{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "se-idag-ai",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"main": "./dist/_worker.js/index.js",
	"assets": {
		"directory": "./dist",
		"binding": "ASSETS"
	},
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"routes": [
		{
			"pattern": "se.idag.ai/*",
			"zone_name": "idag.ai"
		}
	],
	"vars": {
		"GEO_SUBDOMAIN": "se",
		"SITE_LANGUAGE": "sv",
		"SITE_COUNTRY": "Sverige"
	}
}

